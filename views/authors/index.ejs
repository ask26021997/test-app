<h2>Search Authors</h2>

<form action="/authors" method="get">
  <label for="username">Username</label>
  <input
    type="text"
    name="username"
    id="username"
    value="<%= searchOptions.username %>"
  />
  <button type="submit">Search</button>
</form>
<br />
<% authors.forEach( author =>{ %>
<div><%=author.username %></div>
<%})%>
